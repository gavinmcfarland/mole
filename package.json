{
	"name": "mole",
	"version": "1.1.0-alpha.0",
	"description": "A tool for managing design decision outputs for different platforms",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"type": "module",
	"scripts": {
		"jsonnet": "jsonnet src/default-config.jsonnet -o src/default-config.json",
		"build": "webpack",
		"build:rollup": "rollup -c",
		"build:vite": "vite",
		"build:es": "node esbuild.js",
		"build:ts": "tsc",
		"build:tsup": "tsup",
		"build:ncc": "ncc build src/index.js -o dist",
		"test:ncc": "NODE_ENV=test node dist/index.js ",
		"test:es": "NODE_ENV=test node dist/index.js ",
		"test:ts": "NODE_ENV=test node esm/index.js ",
		"test:raw": "NODE_ENV=test node src/index.js"
	},
	"author": "Gavin McFarland",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/limitlessloop/mole.git"
	},
	"files": [
		"dist",
		"src"
	],
	"license": "CC0-1.0",
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@babel/preset-env": "^7.25.3",
		"@rbicker/jsonnet": "^0.1.1-0.12.1",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rtvision/esbuild-dynamic-import": "^2.0.2",
		"babel-loader": "^9.1.3",
		"common-tags": "^1.8.0",
		"esbuild": "^0.23.0",
		"fs-extra": "^11.2.0",
		"jest": "^26.1.0",
		"jsonnet-exec": "^0.1.0",
		"node-addon-jsonnet": "^1.0.17",
		"prettier": "^2.0.5",
		"rollup": "^4.20.0",
		"tsup": "^8.2.4",
		"typescript": "^5.5.4",
		"vite": "^5.3.5",
		"webpack": "^5.93.0",
		"webpack-cli": "^5.1.4"
	},
	"dependencies": {
		"@unboundedsystems/jsonnet": "^0.9.4-rc6",
